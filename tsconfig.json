{
  "compilerOptions": {
    "outDir": "./dist/",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": "src",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "paths": {
      "*": ["*"]
    }
  },
  "include": ["./src/**/*"]
}
